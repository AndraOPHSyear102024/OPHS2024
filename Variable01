email = "andra@abc.co.uk"
name = "andra"
console = "playstation"
age = 100
price = 72.99


#above are five variables 
#one is called "email" 
#other is called "name"
#both contain data


print(email)
print(name)
print(console)
print(age)
print(price)

#print is a function 
#it displays text to the screen
























































































































































































































































